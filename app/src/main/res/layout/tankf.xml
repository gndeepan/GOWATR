										android:textSize="18sp"
										android:textColor="#FFFFFF"
										android:layout_gravity="center_horizontal|center_vertical" />
								</LinearLayout>
								<EditText
									android:id="@+id/sensorr1"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:layout_marginTop="30dp"
									android:padding="8dp"
									android:background="#E0E0E0"
									android:gravity="center_horizontal|center_vertical"
									android:textSize="12sp"
									android:textColor="#000000"
									android:hint="Value In cm"
									android:textColorHint="#607D8B"
									android:inputType="numberDecimal"
									android:layout_gravity="center_horizontal|center_vertical" />
							</LinearLayout>
							<LinearLayout
								android:id="@+id/total1"
								android:layout_width="wrap_content"
								android:layout_height="match_parent"
								android:layout_margin="10dp"
								android:padding="8dp"
								android:gravity="center_horizontal|top"
								android:orientation="vertical">
								<LinearLayout
									android:id="@+id/linear28"
									android:layout_width="300dp"
									android:layout_height="wrap_content"
									android:padding="8dp"
									android:gravity="center_horizontal|center_vertical"
									android:orientation="horizontal"
									android:layout_gravity="center_horizontal|center_vertical">
									<TextView
										android:id="@+id/ta"
										android:layout_width="wrap_content"
										android:layout_height="wrap_content"
										android:padding="8dp"
										android:gravity="center_horizontal|center_vertical"
										android:text="Enter Tank Total Distance"
										android:textSize="18sp"
										android:textColor="#FFFFFF"
										android:layout_gravity="center_horizontal|center_vertical" />
								</LinearLayout>
								<EditText
									android:id="@+id/totall1"
									android:layout_width="wrap_content"
									android:layout_height="wrap_content"
									android:layout_marginTop="30dp"
									android:padding="8dp"
									android:background="#E0E0E0"
									android:gravity="center_horizontal|center_vertical"
									android:textSize="12sp"
									android:textColor="#000000"
									android:hint="Value In cm"
									android:textColorHint="#607D8B"
									android:inputType="numberDecimal"
									android:layout_gravity="center_horizontal|center_vertical" />
							</LinearLayout>
						</LinearLayout>
					</HorizontalScrollView>
					<TextView
						android:id="@+id/finish"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="    Finish    "
						android:textSize="18sp"
						android:textColor="#FFFFFF" />
				</LinearLayout>
			</LinearLayout>
			<LinearLayout
				android:id="@+id/monitory"
				android:layout_width="match_parent"
				android:layout_height="match_parent"
				android:background="@drawable/cyc"
				android:orientation="vertical">
				<LinearLayout
					android:id="@+id/top"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:padding="8dp"
					android:orientation="horizontal">
					<ImageView
						android:id="@+id/drawer"
						android:layout_width="30dp"
						android:layout_height="35dp"
						android:layout_marginTop="10dp"
						android:src="@drawable/menu"
						android:scaleType="fitCenter" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/moni"
					android:layout_width="match_parent"
					android:layout_height="match_parent"
					android:padding="20dp"
					android:background="@android:color/transparent"
					android:gravity="center_horizontal|center_vertical"
					android:orientation="vertical">
					<TextView
						android:id="@+id/title"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="5dp"
						android:gravity="center_horizontal|center_vertical"
						android:text="Go Watr"
						android:textSize="40sp"
						android:textStyle="bold"
						android:textColor="#15096F" />
					<TextView
						android:id="@+id/monitor"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="       "
						android:textSize="20sp"
						android:textColor="#47004E" />
					<LinearLayout
						android:id="@+id/linear1"
						android:layout_width="match_parent"
						android:layout_height="400dp"
						android:padding="8dp"
						android:gravity="center_horizontal|center_vertical"
						android:orientation="vertical"
						android:layout_gravity="center_horizontal|center_vertical" />
					<TextView
						android:id="@+id/distitle"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Percentage"
						android:textSize="16sp"
						android:textColor="#595399" />
					<TextView
						android:id="@+id/lastvalu"
						android:layout_width="wrap_content"
						android:layout_height="wrap_content"
						android:padding="8dp"
						android:text="Loading"
						android:textSize="25sp"
						android:textColor="#15096F" />
				</LinearLayout>
			</LinearLayout>
		</LinearLayout>
	</LinearLayout>
</LinearLayout>
