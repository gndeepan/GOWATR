						android:layout_height="wrap_content"
						android:layout_marginLeft="10dp"
						android:text="About"
						android:textSize="18sp"
						android:textColor="#FFFFFF" />
				</LinearLayout>
				<LinearLayout
					android:id="@+id/linear52"
					android:layout_width="match_parent"
					android:layout_height="wrap_content"
					android:layout_marginTop="35dp"
